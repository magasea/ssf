{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "fund Deails ",
  "description": "基金详情",
  "type": "object",
  "properties": {
    "head": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string"
        },
        "msg": {
          "type": "string"
        }
      },
      "title": "返回头信息"
    },
    "result": {
      "type": "object",
      "properties": {
        "investtype": {
          "type": "string"
        },
        "code": {
          "type": "string"
        },
        "rate": {
          "type": "string"
        },
        "navreturnrankingp": {
          "type": "integer"
        },
        "dayIncrease": {
          "type": "string"
        },
        "classtype": {
          "type": "string"
        },
        "yearIncrease": {
          "type": "string"
        },
        "net": {
          "type": "number"
        },
        "manager": {
          "type": "string"
        },
        "fundcompany": {
          "type": "string"
        },
        "fundsurvey": {
          "type": "string"
        },
        "tradenotice": {
          "type": "string"
        },
        "fundnotice": {
          "type": "string"
        },
        "uplist": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "val": {
                "type": "string"
              },
              "time": {
                "type": "string"
              },
              "type": {
                "type": "string"
              }
            },
            "required": [
              "val",
              "time",
              "type"
            ]
          }
        }
      },
      "required": [
        "investtype",
        "code",
        "rate",
        "navreturnrankingp",
        "dayIncrease",
        "classtype",
        "yearIncrease",
        "uplist",
        "net",
        "manager",
        "fundcompany",
        "fundsurvey",
        "tradenotice",
        "fundnotice"
      ]
    }
  },
  "required": [
    "head",
    "result"
  ]
}